{
  "name": "@plasmo-corp/bms",
  "version": "1.3.0",
  "description": "Browser Market Submission",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:type:emit": "tsc -w --emitDeclarationOnly",
    "dev:type:alias": "tsc-alias -w",
    "dev:compile": "esbuild src/index.ts --bundle --watch --platform=node --outdir=dist",
    "build": "run-p build:type build:compile",
    "build:type": "run-s build:type:*",
    "build:type:emit": "tsc --emitDeclarationOnly",
    "build:type:alias": "tsc-alias",
    "build:compile": "esbuild src/index.ts --bundle --minify --platform=node --outdir=dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "prepare": "run-s clean build"
  },
  "author": "Plasmo Corp. <support@plasmo.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/plasmo-corp/bms.git"
  },
  "keywords": [
    "browser-extension",
    "browser-market",
    "webstore",
    "web-ext",
    "addons",
    "submission",
    "browser-addons",
    "firefox",
    "opera",
    "chrome",
    "edge"
  ],
  "license": "MIT",
  "devDependencies": {
    "@plasmo-corp/rps": "1.1.0",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/node": "17.0.21",
    "esbuild": "0.14.23",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "tsc-alias": "1.6.3",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "@plasmo-corp/cwu": "1.8.0",
    "@plasmo-corp/mwu": "1.6.0",
    "compare-versions": "4.1.3",
    "parse-duration": "1.0.2",
    "puppeteer": "13.4.0",
    "zip-local": "0.3.5"
  }
}
